{
  "manifestVersion": 1,
  "id": "AutoRestNuGetPackageGenerator",
  "version": "1.0.45",
  "name": "AutoRest NuGet Package Generator",
  "scopes": [],
  "description": "Generator for autorest client nuget package",
  "publisher": "eshopworld",
  "public": true,
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "AutoRest",
    "NuGet",
    "Swagger",
    "client",
    "API"
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "screenshots": [
    {
      "path": "AutoRestNuGetGenerator/static/images/Screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "AutoRestNuGetGenerator/license.md"
    },
    "details": {
      "path": "AutoRestNuGetGenerator/overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/eShopWorld/vsts-autorest"
    },
    "support": {
      "uri": "https://github.com/eShopWorld/vsts-autorest"
    },
    "issues": {
      "uri": "https://github.com/eShopWorld/vsts-autorest"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/eShopWorld/vsts-autorest"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "AutoRestNuGetGenerator"
    },
    {
      "path": "ps_modules",
      "packagePath": "AutoRestNuGetGenerator/AutoRestNuGetGenerator/ps_modules"
    }
  ],
  "contributions": [
    {
      "id": "AutoRestNuGetPackageGenerator",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "AutoRestNuGetGenerator"
      }
    }
  ]
}